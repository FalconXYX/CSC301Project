{
  "name": "physiofind-express",
  "version": "0.0.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "prisma generate",
    "migrate:sql": "node ./scripts/runSqlMigration.js",
    "test": "node ./scripts/runTests.js local all",
    "test:prod": "node ./scripts/runTests.js production all",
    "test:clinic": "node ./scripts/runTests.js local clinic",
    "test:clinic:prod": "node ./scripts/runTests.js production clinic",
    "test:user": "node ./scripts/runTests.js local user",
    "test:user:prod": "node ./scripts/runTests.js production user",
    "test:db": "node ./scripts/runTests.js local db",
    "test:db:prod": "node ./scripts/runTests.js production db",
    "test:create-clinic": "node ./scripts/api-testing-scripts/testCreateClinic.js",
    "test:delete-clinic": "node ./scripts/api-testing-scripts/testDeleteClinic.js",
    "test:update-clinic": "node ./scripts/api-testing-scripts/testUpdateClinic.js",
    "test:get-clinics": "node ./scripts/api-testing-scripts/testGetClinics.js",
    "test:get-public-clinics": "node ./scripts/api-testing-scripts/testGetPublicClinics.js",
    "test:db-consistency": "node ./tests/db-consistency-test.js",
    "test:all": "node ./tests/test-runner.js",
    "test:create-user": "node ./tests/api-tests/users/testCreateUser.js",
    "schema:sync": "node ./scripts/syncPrismaSchema.js",
    "db:seed": "node ./scripts/seedDatabase.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.4.3",
    "dotenv": "^16.0.3",
    "express": "~5.2.1",
    "http-errors": "~2.0.1",
    "morgan": "~1.9.1",
    "pg": "^8.12.0",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "prisma": "^5.11.0"
  }
}
